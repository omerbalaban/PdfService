		<Description>Logo Development Platform - Document Pdf Service</Description>
		<Authors>LOGO Group</Authors>
		<TargetFramework>net10.0</TargetFramework>
		<AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
		<CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
		<DebugType>portable</DebugType>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<PreserveCompilationContext>true</PreserveCompilationContext>
		<AssemblyName>eLogo.Micronorm.PdfService.Api</AssemblyName>
		<OutputType>Exe</OutputType>
		<PackageId>PdfService</PackageId>
		<RuntimeIdentifiers>win-x64;linux-x64</RuntimeIdentifiers>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<NeutralLanguage>tr-TR</NeutralLanguage>
		<CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
		<UserSecretsId>10704A48-B982-4376-B6EB-D9C47B0AD7AD</UserSecretsId>

		<RootNamespace>eLogo.PdfService.Api</RootNamespace>
		<StartupObject>eLogo.PdfService.Api.Program</StartupObject>
		<DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
		<Nullable>disable</Nullable>
		<IsTransformWebConfigDisabled>true</IsTransformWebConfigDisabled>
	</PropertyGroup>


	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<PlatformTarget>AnyCPU</PlatformTarget>
		<Optimize>false</Optimize>
	</PropertyGroup>


	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
