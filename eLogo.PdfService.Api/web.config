<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<location path="." inheritInChildApplications="false">
	    <system.webServer>
		    <handlers>
				<add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
	        </handlers>
	        <aspNetCore processPath="dotnet" arguments=".\eLogo.PdfService.Api.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="outofprocess" >
		        <environmentVariables>
			        <environmentVariable name="LBS_DOCKER_HOST_ADDR" value="" />
			        <environmentVariable name="LBS_DOCKER_HOST_PORT" value="80" />
		        </environmentVariables>
	        </aspNetCore>
	    </system.webServer>
    </location>
</configuration>