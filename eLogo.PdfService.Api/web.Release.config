<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<location path="." inheritInChildApplications="false">
		<system.webServer>
			<aspNetCore processPath="%LAUNCHER_PATH%" arguments="%LAUNCHER_ARGS%" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="outofprocess" >
				<environmentVariables>
					<environmentVariable name="LBS_DOCKER_HOST_ADDR" value="#{LBS_DOCKER_HOST_ADDR}" xdt:Transform="Replace" />
					<environmentVariable name="LBS_DOCKER_HOST_PORT" value="#{LBS_DOCKER_HOST_PORT}" xdt:Transform="Replace" />
				</environmentVariables>
			</aspNetCore>
		</system.webServer>
	</location>
</configuration>