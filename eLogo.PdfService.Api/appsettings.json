{
  "PaasSettingsUrl": "http://settings-stg.logo-paas.com",
  "PaasPrefix": "TEST",
  "PaasGroupName": "eLogo Shared Services",
  "ApplicationName": "eLogo.PdfService",
  "SwaggerEnabled": true,
  "Logger": {
    "LogConnectionString": "",
    "FluentdHostName": "fluentd.elogotest.local",
    "FluentdPort": 8888,
    "LogSourceName": "PdfService",
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  // MongoDB Configuration
  "MongoDB": {
    "ConnectionString": "mongodb://localhost:27017",
    "Database": "PdfProvider",
    "LogConnectionString": "mongodb://localhost:27017",
    "LogLevel": 1
  },
  // IronPdf Configuration
  "IronPdf": {
    "SerialKey": "",
    "EnableDebug": false,
    "LogLevel": 0,
    "RenderTimeout": 180,
    "RenderTaskTimeout": 180,
    "ZoomFactor": 95
  },

  // Wkhtml Configuration
  "WkhtmlToPdf": {
    "ServiceUrl": "http://localhost:8000/api/Pdf/ConvertHtmlToPdf",
    "ImageQuality": 70,
    "DPI": 70,
    "ImageDPI": 450
  },

  // PDF Service Settings
  "PdfService": {
    "RequestLimitKB": 5632,
    "DefaultProvider": 10,
    "HtmlTraceEnable": false,
    "HtmlPath": "C:\\HtmlTemp",
    "AuthenticationEnable": true,
    "TransactionLogCounterEnable": true
  },

  // Fluentd Logging Configuration
  "Fluentd": {
    "HostName": "localhost",
    "Port": 24224,
    "SourceName": "PdfService",
    "ProjectCode": "PDFSERVICE"
  }

}
