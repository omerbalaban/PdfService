{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Error",
      "Microsoft.Hosting.Lifetime": "Warning",
      "eLogo.PdfService": "Information"
    }
  },

  // Swagger - Production'da kapal?
  "SwaggerEnabled": false,

  // MongoDB - Production values
  "MongoDB": {
    "ConnectionString": "mongodb://production-server:27017",
    "Database": "PdfProvider",
    "LogConnectionString": "mongodb://production-server:27017",
    "LogLevel": 2
  },

  // IronPdf - Production settings
  "IronPdf": {
    "SerialKey": "PRODUCTION-LICENSE-KEY-HERE",
    "EnableDebug": false,
    "LogLevel": 1,
    "RenderTimeout": 120,
    "RenderTaskTimeout": 120,
    "ZoomFactor": 95
  },

  // Wkhtml - Production URL
  "WkhtmlToPdf": {
    "ServiceUrl": "http://wkhtml-service:8000/api/Pdf/ConvertHtmlToPdf"
  },

  // PDF Service - Production settings
  "PdfService": {
    "HtmlTraceEnable": false,
    "HtmlPath": "/var/log/pdfservice/html",
    "AuthenticationEnable": true,
    "TransactionLogCounterEnable": true
  },

  // Fluentd - Production configuration
  "Fluentd": {
    "HostName": "fluentd-server",
    "Port": 24224,
    "SourceName": "PdfService-Production",
    "ProjectCode": "PDFSERVICE"
  }
}
